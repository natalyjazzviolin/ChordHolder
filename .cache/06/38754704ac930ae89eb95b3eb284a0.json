mn":3216}}],"sources":{"style.css":"body, html {\r\n  margin: 0;\r\n  padding: 0;\r\n  background-color: white;\r\n  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;\r\n}\r\n\r\nh2 {\r\n  /* margin-top: -15vh; */\r\n  font-family: 'Rowdies', cursive;\r\n  font-size: 30px;\r\n  color: rgba(124, 124, 124, 0.582);\r\n\r\n}\r\n\r\na:link, \r\na:visited {\r\n  color: #111;\r\n}\r\n\r\n.container {\r\n  height: 100vh;\r\n  width: 100vw;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  text-align: center;\r\n  /* margin-top: 10vh; */\r\n}\r\n\r\n.footer {\r\n  margin-top: 35vh;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n  font-size: 12px;\r\n}\r\n\r\n.chords {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.chordControls {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: flex-end;\r\n}\r\n\r\n/* .chordContainer {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n} */\r\n\r\n\r\n.chordLabel {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0.7rem;\r\n  padding: 2rem 1rem;\r\n  color: rgba(61, 64, 91);\r\n  transition: 0.2s all;\r\n  cursor: text;\r\n  font-size: 20px;\r\n  text-transform: uppercase;\r\n\r\n}\r\n\r\n\r\n.chordInput {\r\n  max-width: 7rem;\r\n  height: 4rem;\r\n  padding: 1rem;\r\n  margin: 1rem;\r\n  font-size: 30px;\r\n  background-color: white;\r\n  text-align: center;\r\n\r\n  width:100%;\r\n  border: 0;\r\n  outline: 0;\r\n  padding: 0.5rem 0;\r\n  border-bottom: 2px solid rgba(61, 64, 91);\r\n  box-shadow: none;\r\n  color: #111;\r\n}\r\n\r\n\r\n.chordInput:invalid {\r\n  outline: 0;\r\n}\r\n\r\n.chordInput:focus,\r\n.chordInput:valid {\r\n  border-color: rgb(231, 111, 81);\r\n}\r\n\r\n.chordInput:focus~.chordLabel,\r\n.chordInput:valid~.chordLabel{\r\n  font-size: 14px;\r\n  top: -24px;\r\n  color: rgb(231, 111, 81);\r\n}\r\n\r\n.chordInput:valid.chordLabel {\r\n  opacity: 0;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n.chordOne,\r\n .chordTwo {\r\n  position: relative;\r\n  line-height: 5vh;\r\n  display: inline-block;\r\n  \r\n}\r\n\r\n.chordPicker {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.controlButtons {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.resetBtn {\r\n  margin-bottom: -30vh;\r\n  margin-top: 25vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  \r\n}\r\n\r\n.resetIt {\r\n  border: 2px solid rgb(243, 84, 44);\r\n  background-color: rgb(236, 75, 34);\r\n  color: white;\r\n  border-radius: 5px;\r\n  text-transform: uppercase;\r\n  font-size: 30px;\r\n\r\n  -webkit-transition: ease-out .6s;\r\n  transition: ease-out .6s;\r\n\r\n  box-shadow: 0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);\r\n  outline: none;\r\n}\r\n\r\n.resetIt:active {\r\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.486), 0 6px 6px rgba(0,0,0,0.23);\r\n  transform: translateY(10px);\r\n  -webkit-transition: ease-out .4s;\r\n  transition: ease-out .4s;\r\n}\r\n\r\n.controlBtn {\r\n  height: 2rem;\r\n  width: 2rem;\r\n  border-radius: 50%;\r\n  padding: 3rem;\r\n  margin: 30px;\r\n  margin-bottom: -10rem;\r\n\r\n  border: 2px solid rgb(92, 100, 219);\r\n  background-color: rgb(92, 100, 219);\r\n  color: white;\r\n  \r\n  box-shadow: 0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);\r\n\r\n  -webkit-transition: ease-out .4s;\r\n  transition: ease-out .4s;\r\n  outline: none;\r\n}\r\n\r\n.controlBtn:active {\r\n  border: 2px solid rgb(68, 38, 136);\r\n  background-color: rgb(63, 39, 167);\r\n  color: white;\r\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.486), 0 6px 6px rgba(0,0,0,0.23);\r\n  transform: translateY(10px);\r\n  -webkit-transition: ease-out .4s;\r\n  transition: ease-out .4s;\r\n  outline: none;\r\n}\r\n\r\nbutton {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  font-size: 60px;\r\n}\r\n\r\n\r\n\r\n\r\n/* Switch - the box around the slider */\r\n.switch {\r\n  position: relative;\r\n  display: inline-block;\r\n  width: 130px;\r\n  height: 60px;\r\n}\r\n\r\n/* Hide default HTML checkbox */\r\n.switch input {\r\n  opacity: 0;\r\n  width: 0;\r\n  height: 0;\r\n}\r\n\r\n/* Slider */\r\n.slider {\r\n  position: absolute;\r\n  cursor: pointer;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background-color: rgb(92, 100, 219);\r\n  -webkit-transition: .4s;\r\n  transition: .4s;\r\n\r\n  border-radius: 5px;\r\n  box-shadow: 0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);\r\n  outline: none;\r\n}\r\n\r\n.slider:before {\r\n  position: absolute;\r\n  content: \"\";\r\n  height: 50px;\r\n  width: 60px;\r\n  left: 4px;\r\n  bottom: 4px;\r\n  background-color: white;\r\n  -webkit-transition: .4s;\r\n  transition: .4s;\r\n  border-radius: 5px;\r\n}\r\n\r\ninput:checked + .slider {\r\n  background: rgb(92, 100, 219);\r\n\r\n  box-shadow: 0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);\r\n  outline: none;\r\n}\r\n\r\ninput:focus + .slider {\r\n  box-shadow: 0 0 1px rgb(36, 40, 97);\r\n\r\n  box-shadow: 0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);\r\n  outline: none;\r\n}\r\n\r\ninput:checked + .slider:before {\r\n  -webkit-transform: translateX(60px);\r\n  -ms-transform: translateX(60px);\r\n  transform: translate(60px);\r\n}"},"lineCount":null}},"error":null,"hash":"b403ba74cd57ac75517f6caec9451228","cacheData":{"env":{}}}